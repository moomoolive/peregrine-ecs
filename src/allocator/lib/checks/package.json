    "tools": "^0.0.1",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  },
  "keywords": [
    "detect",
    "feature",
    "reflect",
    "typescript",
    "validate"
  ],
  "publishConfig": {
    "access": "public"
  },
  "browser": {
    "process": false,
    "setTimeout": false
  },
  "engines": {
    "node": ">=12.7"
  },
  "files": [
    "*.js",
    "*.d.ts"
  ],

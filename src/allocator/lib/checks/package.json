  "version": "3.1.6",
  "description": "Collection of 50+ type, feature & value checks",
  "type": "module",
  "module": "./index.js",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/thi-ng/umbrella.git"
